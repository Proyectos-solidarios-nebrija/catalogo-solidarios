  <ul>
    <MemberCard name="test" />
    <MemberCard name="test" />
    <MemberCard name="test" />
    <MemberCard name="test" />
    <MemberCard name="test" />
    <MemberCard name="test" />
  </ul>
</div>

<style>
  a {
    color: black;
  }

  h2 {
    margin: 0;
    font-size: 64px;
    display: flex;
    align-items: center;
    font-weight: 400;
  }

  .year-container {
    display: flex;
    align-items: center;
    gap: 50px;
    padding-bottom: 10px;
    border-bottom: 1px solid #d9d9d9;
  }

  ul{
    padding: 0;
    display: flex;
    gap: 45px;
    flex-wrap: wrap;
  }
</style>
