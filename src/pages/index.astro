---
import BaseLayout from '@layouts/BaseLayout.astro'
import Header from '@components/Header.astro'
import Proyects from '@components/Projects.astro'
import Galery from '@components/Gallery.astro'
import LatestNews from '@components/LatestNews.astro'
import { getGallery } from 'src/services/cms'

const galleryDocs = await getGallery()
const gallery = galleryDocs?.docs
---

<BaseLayout>
  <Header />
  <LatestNews />
  <Proyects />
  <Galery gallery={gallery} className="content" />
</BaseLayout>
